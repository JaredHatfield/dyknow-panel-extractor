<Page x:Class="DPX.ManageStudent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="ManageStudent">
    <Grid Height="Auto" Name="grid1" Width="Auto">
        <Label Height="28" Margin="96,15,0,0" Name="label1" VerticalAlignment="Top">Manage Students</Label>
        <ListBox Margin="20,128,0,20" Name="listBoxStudents" MinHeight="300" MinWidth="200" MaxWidth="200" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Button Height="23" Margin="20,60,0,0" Name="buttonRefresh" MinWidth="200" MaxWidth="200" VerticalAlignment="Top" HorizontalAlignment="Left" Click="buttonRefresh_Click">Refresh</Button>
        <TextBox Height="23" Margin="20,95,0,0" Name="textBoxFilter" MinWidth="200" MaxWidth="200" VerticalAlignment="Top" HorizontalAlignment="Left" />
    </Grid>
</Page>
