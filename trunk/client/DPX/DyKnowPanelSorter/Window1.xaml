<Window x:Class="DyKnowPanelSorter.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="DyKnow Panel Sorter" Height="288" Width="629">
    <Grid>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,52,34,0" Name="opendialog" VerticalAlignment="Top" Width="75" Click="opendialog_box">Open</Button>
        <Button HorizontalAlignment="Right" Margin="0,106,34,121" Name="savedialog" Width="75" Click="savedialog_box">Save</Button>
        <Button Margin="168,0,168,71" Name="processsort" Height="23" VerticalAlignment="Bottom" Click="processsort_Click">Process</Button>
        <TextBox Height="23" Margin="0,52,140,0" Name="inputfilename" VerticalAlignment="Top" HorizontalAlignment="Right" Width="324" TextChanged="inputfilename_TextChanged" />
        <TextBox Margin="0,106,140,121" Name="outputfilename" HorizontalAlignment="Right" Width="324" TextChanged="outputfilename_TextChanged" />
        <Label Height="28" HorizontalAlignment="Left" Margin="26,50,0,0" Name="label1" VerticalAlignment="Top" Width="102">Source File</Label>
        <Label HorizontalAlignment="Left" Margin="26,104,0,118" Name="label2" Width="102">Destination File</Label>
    </Grid>
</Window>
